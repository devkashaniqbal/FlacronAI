# FlacronAI Production Environment Variables

# Google Gemini API Key
GEMINI_API_KEY=your_production_gemini_api_key_here

# Firebase Configuration
FIREBASE_PROJECT_ID=flacronai-c8dab
FIREBASE_PRIVATE_KEY_ID=your_production_private_key_id
FIREBASE_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\nyour_production_private_key_here\n-----END PRIVATE KEY-----\n"
FIREBASE_CLIENT_EMAIL=firebase-adminsdk-fbsvc@flacronai-c8dab.iam.gserviceaccount.com
FIREBASE_CLIENT_ID=your_production_client_id

# Server Configuration
PORT=3000
NODE_ENV=production

# Storage Configuration
# Set to 'true' to force Firebase Storage even in development
# In production (NODE_ENV=production), Firebase Storage is used automatically
USE_FIREBASE_STORAGE=false

# Frontend URL (for CORS) - UPDATE THIS TO YOUR PRODUCTION DOMAIN
FRONTEND_URL=https://flacronai.com

# Domain
DOMAIN=flacronai.com

# Stripe Configuration - PRODUCTION KEYS
# Get these from: https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY=sk_live_your_production_stripe_secret_key
STRIPE_WEBHOOK_SECRET=whsec_your_production_webhook_secret

# Stripe Price IDs - Create these in Stripe Dashboard for PRODUCTION
STRIPE_PRICE_PROFESSIONAL=price_your_production_professional_price_id
STRIPE_PRICE_AGENCY=price_your_production_agency_price_id
STRIPE_PRICE_ENTERPRISE=price_your_production_enterprise_price_id
